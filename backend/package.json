{
  "name": "monkey-mind-backend",
  "version": "1.0.0",
  "description": "Backend server for Monkey Mind multiplayer trivia game",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --watch src/server.js",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "seed": "node src/utils/seed.js"
  },
  "keywords": [
    "game",
    "trivia",
    "realtime",
    "socket.io",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "socket.io": "^4.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "socket.io-client": "^4.6.0",
    "supertest": "^6.3.3"
  }
}
